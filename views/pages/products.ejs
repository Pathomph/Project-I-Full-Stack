<% include ../partials/header %>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Price</th>
            <td><a name="" id="" class="btn btn-outline-info" href="#" role="button">ADD</a></td>
        </tr>
    </thead>
    <tbody>
    <% product.forEach(function(product) { %>
        <tr>
        <td scope="row"><%= product.id %></td>

            <td><%= product.title %></td>
            <td><%= product.price %></td>
            <td><a name="" id="" class="btn btn-outline-success" href="/products/<%= product.id%>" role="button">UPDATE</a></td>
        </tr>
        <% });%>
    </tbody>
</table>

  