<% include ../partials/header %>
<form action="/user/update" method="post">
<div class="form-group">
  <label for="">ID</label>
  <input type="number" value="<%= user.id %>" name="id" id="" class="form-control" placeholder="" aria-describedby="helpId">
  <small id="helpId" class="text-muted"></small>
</div>
<div class="form-group">
  <label for="">Email</label>
  <input type="text" value="<%= user.email %>" name="email" id="" class="form-control" placeholder="" aria-describedby="helpId">
  <small id="helpId" class="text-muted"></small>
</div>
<div class="form-group">
  <label for="">Password</label>
  <input type="number" value="<%= user.password %>" name="password" id="" class="form-control" placeholder="" aria-describedby="helpId">
  <small id="helpId" class="text-muted"></small>
</div>

<button type="submit" class="btn btn-success">Update</button>

</form>



 <% include ../partials/footer %>