<% include ../partials/header %>
<form action="/product/update" method="post">
<div class="form-group">
  <label for="">ID</label>
  <input type="number" value="<%= product.id %>" name="id" id="" class="form-control" placeholder="" aria-describedby="helpId">
  <small id="helpId" class="text-muted">Product ID</small>
</div>
<div class="form-group">
  <label for="">Titile</label>
  <input type="text" value="<%= product.title %>" name="title" id="" class="form-control" placeholder="" aria-describedby="helpId">
  <small id="helpId" class="text-muted">Product Title</small>
</div>
<div class="form-group">
  <label for="">Price</label>
  <input type="number" value="<%= product.price %>" name="price" id="" class="form-control" placeholder="" aria-describedby="helpId">
  <small id="helpId" class="text-muted">Product Price</small>
</div>

<button type="submit" class="btn btn-success">Update</button>

</form>



 <% include ../partials/footer %>